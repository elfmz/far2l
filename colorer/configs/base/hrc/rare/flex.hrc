      <scheme name="flex">
         <block start="/\s* \M \$\$/x" end="/$/" scheme="flexPreprocess"/>
         <block start="/\/\//" end="/$/" scheme="Comment" region="Comment"/>
         <inherit scheme="def:Number"/>
         <inherit scheme="CString"/>
         <inherit scheme="PairedBrackets">
            <virtual scheme="PairedBrackets" subst-scheme="flex"/>
         </inherit>
         <block start="/\b(Begin)\b/ix" end="/\b(End)\b/ix" scheme="flex" region00="PairStart" region10="PairEnd" region01="flexKeyword" region11="flexKeyword"/>
         <regexp match="/\bExternal \s+ Function\b/ix" region="flexMethods"/>
         <block start="/\b((Function)) \s+(\w+)\b/ix" end="/\b(End \s+ Function)\b/ix" scheme="flex" region02="PairStart" region10="PairEnd" region03="Function" region01="flexKeyword" region11="flexKeyword"/>
         <block start="/\b((Event)) \s+(\w+)\b/ix" end="/\b(End \s+ Event)\b/ix" scheme="flex" region02="PairStart" region10="PairEnd" region03="Function" region01="flexKeyword" region11="flexKeyword"/>
         <block start="/\b(Select \s+ Case)\b/ix" end="/\b(End \s+ Select)\b/ix" scheme="flex" region00="PairStart" region10="PairEnd" region01="flexKeyword" region11="flexKeyword"/>
         <block start="/\b(Structure)\b/ix" end="/\b(EndStruct)\b/ix" scheme="flex" region00="PairStart" region10="PairEnd" region01="flexKeyword" region11="flexKeyword"/>
         <block start="/\b(object)\b/ix" end="/\b(Endobject)\b/ix" scheme="flex" region00="PairStart" region10="PairEnd" region01="flexKeyword" region11="flexKeyword"/>
         <inherit scheme="flexKeywords"/>
         <keywords region="Symbol">
            <symb name="."/>
            <symb name="&amp;"/>
            <symb name="+"/>
            <symb name="-"/>
            <symb name="/"/>
            <symb name="%"/>
            <symb name="*"/>
            <symb name="="/>
            <symb name="&gt;&gt;"/>
            <symb name="&gt;"/>
            <symb name="&lt;"/>
            <word name="AND"/>
            <word name="OR"/>
            <word name="NOT"/>
            <word name="XOR"/>
         </keywords>
      </scheme>

