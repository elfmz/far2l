   <type name="flex">

      <import type="def"/>

      <region name="flexKeyword" parent="Keyword"/>
      <region name="flexProperties" parent="KeywordStrong"/>
      <region name="flexStructMembers" parent="KeywordStrong"/>
      <region name="flexMethods" parent="Keyword"/>


      <scheme name="flexPreprocess">
         <keywords region="Directive" ignorecase="yes">
            <word name="$$DEFINE"/>
            <word name="$$ELSEIF"/>
            <word name="$$ENDIF"/>
            <word name="$$FLEX_INCLUDE"/>
            <word name="$$IFDEF"/>
            <word name="$$INCLUDE"/>
            <word name="$$INTERFACE"/>
            <word name="$$RESOURCE"/>
            <word name="is" region="Keyword"/>
         </keywords>
         <block start="/\/\//" end="/$/" scheme="Comment" region="Comment"/>
         <inherit scheme="def:Number"/>
         <inherit scheme="CString"/>
      </scheme>

