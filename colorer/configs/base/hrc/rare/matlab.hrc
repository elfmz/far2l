            <word name="zeros"/>
            <word name="ones"/>
            <word name="rand"/>
            <word name="randn"/>
            <word name="end"/>
            <word name="ind2sub"/>
            <word name="sub2ind"/>
            <word name="repmat"/>
            <word name="cat"/>
            <word name="ndims"/>
