            <word name="vectorsize"/>
            <word name="class"/>
            <word name="isa"/>
            <word name="isobject"/>
            <word name="methods"/>
            <word name="inferioto"/>
            <word name="superioto"/>
            <word name="horzcat"/>
            <word name="vertcat"/>
            <word name="subsref"/>
            <word name="subsasgn"/>
            <word name="subsindex"/>
         </keywords>
<!-- Misc functions -->
         <keywords ignorecase="yes" region="mlStdFunc">
            <word name="ans"/>
            <word name="inf"/>
            <word name="NaN"/>
            <word name="Pi"/>
            <word name="eps"/>
            <word name="log2"/>
