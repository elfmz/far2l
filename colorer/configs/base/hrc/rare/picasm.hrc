            <word name="pie2"/>

            <word name="porta"/>
            <word name="trisa"/>
            <word name="lata"/>
            <word name="portb"/>
            <word name="trisb"/>
            <word name="latb"/>
            <word name="portc"/>
            <word name="trisc"/>
            <word name="latc"/>
            <word name="portd"/>
            <word name="trisd"/>
            <word name="latd"/>
            <word name="porte"/>
            <word name="trise"/>
            <word name="late"/>
            <word name="portf"/>
            <word name="trisf"/>
            <word name="latf"/>

            <word name="option_reg"/>
            <word name="pcon"/>
         </keywords>
<!-- Symbols -->
         <keywords ignorecase="yes" region="asmSymbol">
            <symb name="="/>
            <symb name="+"/>
            <symb name="-"/>
            <symb name="/"/>
            <symb name="*"/>
            <symb name="!"/>
            <symb name="~"/>
            <symb name="("/>
            <symb name=")"/>
            <symb name=""/>
            <symb name="&lt;"/>
            <symb name="&lt;&lt;"/>
            <symb name="&lt;="/>
            <symb name="&gt;"/>
            <symb name="&gt;&gt;"/>
            <symb name="&gt;="/>
            <symb name="=="/>
            <symb name=","/>
            <symb name="&amp;"/>
            <symb name="&amp;&amp;"/>
            <symb name="|"/>
            <symb name="||"/>
            <symb name="+="/>
            <symb name="-="/>
            <symb name="*="/>
