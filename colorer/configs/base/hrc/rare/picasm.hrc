            <word name="ccpr2l"/>
            <word name="ccpr2h"/>
            <word name="ccp2con"/>

            <word name="spbrg"/>
            <word name="rcreg"/>
            <word name="txreg"/>
            <word name="rcsta"/>
            <word name="txsta"/>

            <word name="eeadr"/>
            <word name="eedata"/>
            <word name="eecon2"/>
            <word name="eecon1"/>

            <word name="ipr1"/>
            <word name="pir1"/>
            <word name="pie1"/>
            <word name="ipr2"/>
