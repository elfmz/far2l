            <word name="movlb"/>
            <word name="movlw"/>
            <word name="movwf"/>
            <word name="mulwf"/>
            <word name="mullw"/>
            <word name="mulwf"/>
            <word name="negf"/>
            <word name="nop"/>
            <word name="pagesel"/>
            <word name="pageselw"/>
            <word name="pop"/>
            <word name="push"/>
            <word name="rcall"/>
            <word name="reset"/>
            <word name="retfie"/>
            <word name="retlw"/>
            <word name="return"/>
            <word name="rlcf"/>
            <word name="rlncf"/>
            <word name="rrcf"/>
            <word name="rrncf"/>
            <word name="setf"/>
            <word name="sleep"/>
            <word name="subfwb"/>
