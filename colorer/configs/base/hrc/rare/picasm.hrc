            <word name="addfsr"/>
            <word name="addulnk"/>
            <word name="callw"/>
            <word name="movsf"/>
            <word name="movss"/>
            <word name="pushl"/>
            <word name="subfsr"/>
            <word name="subulnk"/>
         </keywords>
<!-- asm registers -->
         <keywords ignorecase="yes" region="asmRegister">
            <word name="tosu"/>
            <word name="tosh"/>
