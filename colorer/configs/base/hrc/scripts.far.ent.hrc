    <prototype name="farhelp" group="scripts.far" description="Far Help">
        <location link="scripts/far/farhelp.hrc" />
        <filename>/\.hlf$/i</filename>
        <firstline weight="2">/^\.Language/</firstline>
    </prototype>

    <prototype name="farlng" group="scripts.far" description="Far .lng">
        <location link="scripts/far/farlng.hrc" />
        <filename>/\.lng$/i</filename>
        <firstline weight="2">/^\.Language/</firstline>
    </prototype>

    <prototype name="farmenu" group="scripts.far" description="FarMenu.Ini">
        <location link="scripts/far/farmenu.hrc" />
        <filename weight="4">/FarMenu\.Ini$/i</filename>
    </prototype>

    <prototype name="farmailscript" group="scripts.far" description="FARMail Script">
        <location link="scripts/far/farmailscript.hrc" />
        <filename>/\.fms$/i</filename>
    </prototype>

    <prototype name="far.macro" group="scripts.far" description="Far macros">
        <location link="scripts/far/macro.hrc" />
        <filename>/\.macro$/i</filename>
        <filename>/\.mcr$/i</filename>
        <filename>/\.fmm$/i</filename>
        <filename>/^Sequence$/</filename>
        <filename>/^\%\%\w+$/</filename>
    </prototype>

    <prototype name="far.macrolib" group="scripts.far" description="Far Macro Library">
        <location link="scripts/far/macrolib.hrc" />
        <filename>/\.fm[li]$/i</filename>
    </prototype>
    <prototype name="far.macroliblua" group="scripts.far" description="Far Macro Library (Lua)">
        <location link="scripts/far/macroliblua.hrc" />
        <filename>/\.fmlua$/i</filename>
    </prototype>

    <prototype name="calcset" group="scripts.far" description="calc's defines">
        <location link="scripts/gen/far/calcset.hrc" />
        <filename>/calcset\.csr$/i</filename>
    </prototype>


    <prototype name="esc" group="scripts.far" description="[ESC] Editor's settings changer">
        <location link="scripts/gen/far/esc.hrc" />
        <filename weight='3'>/^esc(_\w+)?\.xml$/i</filename>
    </prototype>

    <!-- old -->

    <prototype name="tgs" group="scripts.far" description="TGS script">
        <location link="scripts/far/tgs.hrc" />
        <filename>/\.tgs$/i</filename>
    </prototype>

    <prototype name="truemac" group="scripts.far" description="True Macro">
        <location link="scripts/far/truemac.hrc" />
        <filename>/truemac\.ctl$/i</filename>
    </prototype>
