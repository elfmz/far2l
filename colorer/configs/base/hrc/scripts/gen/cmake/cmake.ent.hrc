</scheme>

<scheme name="_HASH_">
	<keywords region="cmake_parameters" ignorecase="yes">
		<!-- Hashing -->
		<word name="MD5"/>
		<word name="SHA1"/>
		<word name="SHA224"/>
		<word name="SHA256"/>
		<word name="SHA384"/>
		<word name="SHA512"/>
		<word name="SHA3_224"/>
		<word name="SHA3_256"/>
		<word name="SHA3_384"/>
		<word name="SHA3_512"/>
	</keywords>
</scheme>
