      </scheme>
      <scheme name="hrd-entry-elementContent">
         <inherit scheme="_hrc_int_elementContent">
            <virtual scheme="xml:element" subst-scheme="hrd-entry-content"/>
            <virtual scheme="xml:content.cdata" subst-scheme="xml:badChar"/>
