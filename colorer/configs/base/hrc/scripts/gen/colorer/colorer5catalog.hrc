            <virtual scheme="xml:Attribute.any" subst-scheme="catalog-Attributes"/>
         </inherit>
      </scheme>
      <scheme name="hrc-sets-content">
         <block start="/\M &lt; (%nsprefix;location ([\s\/&gt;]|$) )/x" end="/ &gt; /x"
