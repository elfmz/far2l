      </scheme>
      <!--custom schemes from 'custom.default.xml'--><!--end custom--><scheme name="catalog-element">
         <block start="/\M &lt; (%nsprefix;catalog ([\s\/&gt;]|$) )/x" end="/ &gt; /x"
