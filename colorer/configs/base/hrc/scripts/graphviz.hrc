        <word name="shape"/>
        <word name="shapefile"/>
        <word name="showboxes"/>
        <word name="sides"/>
        <word name="size"/>
        <word name="skew"/>
        <word name="smoothing"/>
        <word name="sortv"/>
        <word name="splines"/>
        <word name="start"/>
        <word name="style"/>
        <word name="stylesheet"/>
        <word name="tailURL"/>
        <word name="tailclip"/>
        <word name="tailhref"/>
        <word name="taillabel"/>
        <word name="tailport"/>
        <word name="tailtarget"/>
        <word name="tailtooltip"/>
        <word name="target"/>
        <word name="tooltip"/>
        <word name="truecolor"/>
        <word name="vertices"/>
        <word name="viewport"/>
        <word name="voro_margin"/>
        <word name="weight"/>
        <word name="width"/>
        <word name="z"/>
        <symb name=","/>
        <symb name="="/>
      </keywords>
      <inherit scheme="def:Number"/>
      <inherit scheme="node_label"/>

    </scheme>


  </type>

</hrc>
