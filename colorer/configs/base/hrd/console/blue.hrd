<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hrd PUBLIC "-//Cail Lomecb//DTD Colorer HRD take5//EN"
  "http://colorer.sf.net/2003/hrd.dtd">
<hrd xmlns="http://colorer.sf.net/2003/hrd">

  <assign name="def:Text" fore="#f" back="#1"/>
  <assign name="def:HorzCross"  fore="#f" back="#0"/>
  <assign name="def:VertCross" fore="#f" back="#0"/>
  <assign name="def:Syntax"/>

  <assign name="def:Number"    fore="#A"/>
  <assign name="def:NumberSuffix" fore="#2"/>

  <assign name="def:String" fore="#B"/>
  <assign name="def:StringContent" fore="#3"/>
  <assign name="def:StringEdge" fore="#C"/>
  <assign name="def:CharacterContent" fore="#4"/>

  <assign name="def:Comment" fore="#3"/>
  <assign name="def:CommentContent" fore="#7"/>
  <assign name="def:CommentEdge"    fore="#7"/>
  <assign name="def:CommentDoc"     fore="#3"/>
  <assign name="def:CommentDocContent" fore="#7"/>
  <assign name="def:CommentDocEdge" fore="#7"/>

  <assign name="def:Symbol" fore="#E"/>
  <assign name="def:SymbolStrong" fore="#D"/>
  <assign name="def:Prefix" fore="#E"/>
  <assign name="def:PrefixStrong" fore="#D"/>

  <assign name="def:Operator" fore="#E"/>

  <assign name="def:Keyword"         fore="#E"/>
  <assign name="def:TypeKeyword"   fore="#6"/>

  <assign name="def:Function"/>
  <assign name="def:Register" fore="#B"/>
  <assign name="def:Constant" fore="#A"/>
  <assign name="def:BooleanConstant" fore="#E"/>
  <assign name="def:Var" fore="#F"/>
  <assign name="def:VarStrong" fore="#2"/>
  <assign name="def:Identifier" fore="#2"/>

  <assign name="def:Directive" fore="#A"/>
  <assign name="def:Parameter" fore="#A"/>

  <assign name="def:Tag" fore="#7"/>
  <assign name="def:OpenTag" fore="#E"/>
  <assign name="def:CloseTag" fore="#E"/>

  <assign name="def:Label" fore="#D"/>
  <assign name="def:LabelStrong" fore="#1" back="#F"/>

  <assign name="def:Insertion" fore="#F" back="#0"/>

  <assign name="def:Error" fore="#0" back="#4"/>
  <assign name="def:ErrorText" fore="#D"/>

  <assign name="def:TODO" fore="#F" back="#9"/>
  <assign name="def:Debug" fore="#C" back="#6"/>

  <assign name="def:URI" fore="#C"/>
  <assign name="def:EMail" fore="#6"/>

  <assign name="def:Date" fore="#E"/>
  <assign name="def:Time" fore="#A"/>

  <assign name="def:PairStart" fore="#4"/>
  <assign name="def:PairEnd" fore="#4"/>
  <assign name="def:PairStrongStart" fore="#E" back="#9"/>
  <assign name="def:PairStrongEnd" fore="#E" back="#9"/>

</hrd>