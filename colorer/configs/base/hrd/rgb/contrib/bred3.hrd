  <assign name="def:NumberHex" fore="#c00000"/>
  <assign name="def:NumberBin" fore="#005b00"/>
  <assign name="def:NumberOct" fore="#008c00"/>
  <assign name="def:NumberFloat" fore="#800000"/>
  <assign name="def:NumberSuffix" fore="#006600"/>

  <assign name="def:String" fore="#e60000"/>
  <assign name="def:StringContent" fore="#0f6900"/>
  <assign name="def:StringEdge" fore="#800000"/>
  <assign name="def:Character" fore="#008000"/>
  <assign name="def:CharacterContent" fore="#00cc00"/>

  <assign name="def:Comment" fore="#c34e00"/>
  <assign name="def:CommentContent" fore="#7F9FBF" style='1'/>
  <assign name="def:CommentEdge" fore="#606060"/>
  <assign name="def:CommentDoc" fore="#3F5FBF"/>
  <assign name="def:CommentDocEdge" fore="#c0bd92"/>

  <assign name="def:Symbol" fore="#806030"/>
