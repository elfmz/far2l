<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hrd PUBLIC "-//Cail Lomecb//DTD Colorer HRD take5//EN"
  "http://colorer.sf.net/2003/hrd.dtd">
<hrd xmlns="http://colorer.sf.net/2003/hrd">
  <documentation>
    Bred 3 TwilightBlue from Klug
  </documentation>

  <assign name="def:Text" fore="#000000" back="#f1f0f0"/>
  <assign name="def:HorzCross" back="#ebe8e8"/>
  <assign name="def:VertCross" back="#ebe8e8"/>

  <assign name="def:Number" fore="#8c0000"/>
