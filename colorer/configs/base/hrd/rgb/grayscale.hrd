  <assign name="def:Error" fore="#ffffff" back="#494949" style='1'/>
  <assign name="def:ErrorText" fore="#9e9e9e" />
  <assign name="def:TODO" fore="#ffffff" back="#555555" style='1'/>
  <assign name="def:Debug" fore="#b8b8b8" back="#515151" style='1'/>

  <assign name="def:Path" fore="#333333" />
  <assign name="def:URL" fore="#444444" />
  <assign name="def:EMail" fore="#676767" />

  <assign name="def:Date" fore="#646464" />
  <assign name="def:Time" fore="#797979" />

  <assign name="def:PairStart" fore="#333333"/>
  <assign name="def:PairEnd" fore="#333333"/>

</hrd>