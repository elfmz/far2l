system nt_dll initi termi
option version=2.1
option quiet
option case
option eliminate
option showd
option vfr
option heap=4
option stack=8k
option nostdcall
option statics
option offset=0x10000000
option nodefaultlibs
runtime console=4.0
# ref __DLLstart_
lib user32.lib
lib kernel32.lib
lib advapi32.lib

#export Configure.0='_Configure@4'
#export GetPluginInfo.1='_GetPluginInfo@4'
#export OpenPlugin.2='_OpenPlugin@8'
#export SetStartupInfo.3='_SetStartupInfo@4'
